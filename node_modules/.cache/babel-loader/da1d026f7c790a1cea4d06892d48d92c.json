return states[i][1];\n          }\n        }\n      } else if (to == 'name') {\n        input = input.toUpperCase();\n\n        for (let i = 0; i < states.length; i++) {\n          if (states[i][1] == input) {\n            return states[i][0];\n          }\n        }\n      }\n    },\n\n    getDOTInformation(usdot) {\n      const self = this;\n      fetch(`https://mobile.fmcsa.dot.gov/qc/services/carriers/id/${usdot}?webKey=27b663ed331bff071a9f02ba8d726e3558a665aa`).then(response => response.json()).then(function (data) {\n        let obj = data.content;\n        obj.phyState = self.abbrState(obj.phyState, 'name'); // Temporary Values\n\n        obj.ownerFirstName = \"Luke\";\n        obj.ownerLastName = \"David\";\n        obj.ownerEmail = \"luke@giganticdesign.com\";\n        obj.ownerPhone = \"563-580-7440\";\n        obj.contactFirstName = \"\";\n        obj.contactLastName = \"\";\n        obj.contactEmail = \"\";\n        obj.contactPhone = \"\";\n        obj.vins = ['3ALXA7005JDJP9024', '3ALXA7007LDMM7204', '3ALXA7009LDMM7205', '3ALXA7004HDHZ1163', '3ALXA7009JDJP9026', '3ALXA7009GDHD7190', '3ALXA7002GDHD7189', '1FVXA7CG1GLHD7202', '1FUJA6CKX6LV85137'];\n        self.$store.commit('updateDOTData', obj);\n      }).then(function () {\n        self.$router.push({\n          name: 'verifyDOT'\n        });\n      });\n    },\n\n    setRoutes() {\n      const self = this;\n      const routes = this.$router.getRoutes();\n      let route_index = 0;\n      this.curr_route = this.$router.currentRoute.value;\n      routes.forEach(function (route, index) {\n        if (route.name === self.curr_route.name) {\n          route_index = index;\n        }\n      });\n      this.route_index = route_index;\n    }\n\n  },\n\n  mounted() {\n    this.setRoutes();\n  }\n\n};","map":{"version":3,"mappings":"AAgBA,eAAe;EACdA,IAAI,EAAE,UADQ;;EAEdC,IAAI,GAAG;IACN,OAAO,EAAP;EAGA,CANa;;EAOdC,QAAQ,EAAE;IACTC,YAAY,EAAE;MACbC,GAAG,GAAG;QACL,OAAO,KAAKC,MAAL,CAAYF,YAAnB;MACA,CAHY;;MAIbG,GAAG,CA