{"ast":null,"code":"import { mapState } from 'vuex';\nexport default {\n  name: 'TractorVINs',\n\n  data() {\n    return {\n      added_vin: ''\n    };\n  },\n\n  computed: { ...mapState({\n      usdot_data: state => state.usdot_data\n    }),\n    vin_data: {\n      get() {\n        return this.$store.state.vin_data;\n      },\n\n      set(value) {\n        this.$store.commit('updateVinData', value);\n      }\n\n    }\n  },\n  methods: {\n    addVIN() {\n      if (this.added_vin.length > 0) {\n        if (!this.vinIsDuplicate(this.added_vin)) {\n          this.vin_data.push(this.vinLookup(this.added_vin));\n          this.added_vin = '';\n        }\n      }\n    },\n\n    vinIsDuplicate(vin) {\n      const matches = this.vin_data.filter(vechicle => {\n        return vechicle.vin === vin;\n      });\n      return matches.length > 0 ? true : false;\n    },\n\n    removeVIN(vin) {\n      this.vin_data.forEach((vehicle, index) => {\n        if (vehicle.vin === vin) {\n          index > -1 ? this.vin_data.splice(index, 1) : false;\n        }\n      });\n    },\n\n    vinLookup(vin) {\n      const vin_data = {\n        vin: vin,\n        make: String,\n        model: String,\n        year: String\n      };\n      fetch(`https://vpic.nhtsa.dot.gov/api/vehicles/DecodeVin/${vin}?format=json`).then(response => response.json()).then(function (data) {\n        data.Results.forEach(element => {\n          if (element.Variable === 'Make') vin_data.make = element.Value;\n          if (element.Variable === 'Model') vin_data.model = element.Value;\n          if (element.Variable === 'Model Year') vin_data.year = element.Value;\n        });\n      }); // console.log(vin_data);\n\n      return vin_data;\n    },\n\n    getAllVinData() {\n      const self = this;\n      let vins = [];\n      this.usdot_data.vins.forEach(function (vin) {\n        vins.push(self.vinLookup(vin));\n      });\n      this.$store.commit('updateVinData', vins);\n    }\n\n  },\n\n  mounted() {\n    this.getAllVinData();\n  }\n\n};","map":{"version":3,"mappings":"AAsBA,SAASA,QAAT,QAAyB,MAAzB;AAEA,eAAe;EACdC,IAAI,EAAE,aADQ;;EAEdC,IAAI,GAAE;IACL,OAAO;MACNC,SAAS,EAAE;IADL,CAAP;EAGA,CANa;;EAOdC,QAAQ,EAAE,EACT,GAAGJ,QAAQ,CAAC;MACXK,UAAU,EAAEC,KAAI,IAAKA,KAAK,CAACD;IADhB,CAAD,CADF;IAITE,QAAQ,EAAE;MACTC,GAAG,GAAE;QACJ,OAAO,KAAKC,MAAL,CAAYH,KAAZ,CAAkBC,QAAzB;MACA,CAHQ;;MAITG,GAAG,CAACC,KAAD,EAAO;QACT,KAAKF,MAAL,CAAYG,MAAZ,CAAmB,eAAnB,EAAoCD,KAApC;MACD;;IANS;EAJD,CAPI;EAoBdE,OAAO,EAAE;IACRC,MAAM,GAAE;MACP,IAAG,KAAKX,SAAL,CAAeY,MAAf,GAAwB,CAA3B,EAA8B;QAC7B,IAAG,CAAC,KAAKC,cAAL,CAAoB,KAAKb,SAAzB,CAAJ,EAAyC;UACxC,KAAKI,QAAL,CAAcU,IAAd,CAAmB,KAAKC,SAAL,CAAe,KAAKf,SAApB,CAAnB;UACA,KAAKA,SAAL,GAAiB,EAAjB;QACD;MACD;IACA,CARO;;IASRa,cAAc,CAACG,GAAD,EAAK;MAClB,MAAMC,OAAM,GAAI,KAAKb,QAAL,CAAcc,MAAd,CAAqBC,QAAO,IAAK;QAChD,OAAOA,QAAQ,CAACH,GAAT,KAAiBA,GAAxB;MACA,CAFe,CAAhB;MAGA,OAAOC,OAAO,CAACL,MAAR,GAAiB,CAAjB,GAAqB,IAArB,GAA4B,KAAnC;IACA,CAdO;;IAeRQ,SAAS,CAACJ,GAAD,EAAM;MACd,KAAKZ,QAAL,CAAciB,OAAd,CAAsB,CAACC,OAAD,EAAUC,KAAV,KAAoB;QACzC,IAAID,OAAO,CAACN,GAAR,KAAgBA,GAApB,EAAyB;UACxBO,KAAI,GAAI,CAAC,CAAT,GAAa,KAAKnB,QAAL,CAAcoB,MAAd,CAAqBD,KAArB,EAA4B,CAA5B,CAAb,GAA8C,KAA9C;QACD;MACA,CAJD;IAKA,CArBO;;IAsBRR,SAAS,CAACC,GAAD,EAAK;MACb,MAAMZ,QAAO,GAAI;QAChBY,GAAG,EAAEA,GADW;QAEhBS,IAAI,EAAEC,MAFU;QAGhBC,KAAK,EAAED,MAHS;QAIhBE,IAAI,EAAEF;MAJU,CAAjB;MAMAG,KAAK,CAAE,qDAAoDb,GAAI,cAA1D,CAAL,CACCc,IADD,CACMC,QAAO,IAAKA,QAAQ,CAACC,IAAT,EADlB,EAECF,IAFD,CAEM,UAAS/B,IAAT,EAAc;QACnBA,IAAI,CAACkC,OAAL,CAAaZ,OAAb,CAAqBa,OAAM,IAAK;UAC/B,IAAGA,OAAO,CAACC,QAAR,KAAqB,MAAxB,EAAgC/B,QAAQ,CAACqB,IAAT,GAAgBS,OAAO,CAACE,KAAxB;UAChC,IAAGF,OAAO,CAACC,QAAR,KAAqB,OAAxB,EAAiC/B,QAAQ,CAACuB,KAAT,GAAiBO,OAAO,CAACE,KAAzB;UACjC,IAAGF,OAAO,CAACC,QAAR,KAAqB,YAAxB,EAAsC/B,QAAQ,CAACwB,IAAT,GAAgBM,OAAO,CAACE,KAAxB;QACtC,CAJD;MAKA,CARD,EAPa,CAgBb;;MACA,OAAOhC,QAAP;IACA,CAxCO;;IAyCRiC,aAAa,GAAE;MACd,MAAMC,IAAG,GAAI,IAAb;MACA,IAAIC,IAAG,GAAI,EAAX;MACA,KAAKrC,UAAL,CAAgBqC,IAAhB,CAAqBlB,OAArB,CAA6B,UAASL,GAAT,EAAa;QACzCuB,IAAI,CAACzB,