{"ast":null,"code":"export default {\n  name: 'TractorVINs',\n\n  data() {\n    return {\n      added_vin: ''\n    };\n  },\n\n  computed: {\n    vin_data: {\n      get() {\n        return this.$store.state.vin_data;\n      },\n\n      set(value) {\n        this.$store.commit('updateVinData', value);\n      }\n\n    }\n  },\n  methods: {\n    addVIN() {\n      if (this.added_vin.length > 0 && !this.vinIsDuplicate(this.added_vin) && this.vinIsValid(this.added_vin)) {\n        this.vinLookup(this.added_vin);\n        this.added_vin = '';\n      }\n    },\n\n    vinIsDuplicate(vin) {\n      const matches = this.vin_data.filter(vechicle => {\n        return vechicle.vin === vin;\n      });\n      return matches.length > 0 ? true : false;\n    },\n\n    removeVIN(vin) {\n      this.vin_data.forEach((vehicle, index) => {\n        if (vehicle.vin === vin) {\n          index > -1 ? this.vin_data.splice(index, 1) : false;\n        }\n      });\n    },\n\n    vinIsValid(vin) {\n      var re = new RegExp(\"^[A-HJ-NPR-Z\\\\d]{8}[\\\\dX][A-HJ-NPR-Z\\\\d]{2}\\\\d{6}$\");\n      return vin.match(re);\n    },\n\n    vinLookup(vin) {\n      const self = this;\n      const vin_data = {\n        vin: vin,\n        make: String,\n        model: String,\n        year: String\n      };\n      fetch(`https://vpic.nhtsa.dot.gov/api/vehicles/DecodeVin/${vin}?format=json`).then(response => response.json()).then(function (data) {\n        console.log('got data');\n        data.Results.forEach(element => {\n          if (element.Variable === 'Make') vin_data.make = element.Value;\n          if (element.Variable === 'Model') vin_data.model = element.Value;\n          if (element.Variable === 'Model Year') vin_data.year = element.Value;\n        });\n        self.$store.commit('addVin', vin_data);\n      });\n    },\n\n    getAllVinData() {\n      const self = this;\n      this.usdot_data.vins.forEach(vin => self.vinLookup(vin));\n    }\n\n  },\n\n  mounted() {\n    if (this.vin_data.length === 0) {\n      this.getAllVinData();\n    }\n  }\n\n};","map":{"version":3,"mappings":"AA2BA,eAAe;EACdA,IAAI,EAAE,aADQ;;EAEdC,IAAI,GAAE;IACL,OAAO;MACNC,SAAS,EAAE;IADL,CAAP;EAGA,CANa;;EAOdC,QAAQ,EAAE;IACTC,QAAQ,EAAE;MACTC,GAAG,GAAE;QACJ,OAAO,KAAKC,MAAL,CAAYC,KAAZ,CAAkBH,QAAzB;MACA,CAHQ;;MAITI,GAAG,CAACC,KAAD,EAAO;QACT,KAAKH,MAAL,CAAYI,MAAZ,CAAmB,eAAnB,EAAoCD,KAApC;MACD;;IANS;EADD,CAPI;EAiBdE,OAAO,EAAE;IACRC,MAAM,GAAE;MACP,IAAG,KAAKV,SAAL,CAAeW,MAAf,GAAwB,CAAxB,IAA6B,CAAC,KAAKC,cAAL,CAAoB,KAAKZ,SAAzB,CAA9B,IAAqE,KAAKa,UAAL,CAAgB,KAAKb,SAArB,CAAxE,EAAyG;QACxG,KAAKc,SAAL,CAAe,KAAKd,SAApB;QACA,KAAKA,SAAL,GAAiB,EAAjB;MACD;IACA,CANO;;IAORY,cAAc,CAACG,GAAD,EAAK;MAClB,MAAMC,OAAM,GAAI,KAAKd,QAAL,CAAce,MAAd,CAAqBC,QAAO,IAAK;QAChD,OAAOA,QAAQ,CAACH,GAAT,KAAiBA,GAAxB;MACA,CAFe,CAAhB;MAGA,OAAOC,OAAO,CAACL,MAAR,GAAiB,CAAjB,GAAqB,IAArB,GAA4B,KAAnC;IACA,CAZO;;IAaRQ,SAAS,CAACJ,GAAD,EAAM;MACd,KAAKb,QAAL,CAAckB,OAAd,CAAsB,CAACC,OAAD,EAAUC,KAAV,KAAoB;QACzC,IAAID,OAAO,CAACN,GAAR,KAAgBA,GAApB,EAAyB;UACxBO,KAAI,GAAI,CAAC,CAAT,GAAa,KAAKpB,QAAL,CAAcqB,MAAd,CAAqBD,KAArB,EAA4B,CAA5B,CAAb,GAA8C,KAA9C;QACD;MACA,CAJD;IAKA,CAnBO;;IAoBRT,UAAU,CAACE,GAAD,EAAK;MACd,IAAIS,EAAC,GAAI,IAAIC,MAAJ,CAAW,oDAAX,CAAT;MACA,OAAOV,GAAG,CAACW,KAAJ,CAAUF,EAAV,CAAP;IACA,CAvBO;;IAwBRV,SAAS,CAACC,GAAD,EAAK;MACb,MAAMY,IAAG,GAAI,IAAb;MACA,MAAMzB,QAAO,GAAI;QAChBa,GAAG,EAAEA,GADW;QAEhBa,IAAI,EAAEC,MAFU;QAGhBC,KAAK,EAAED,MAHS;QAIhBE,IAAI,EAAEF;MAJU,CAAjB;MAMAG,KAAK,CAAE,qDAAoDjB,GAAI,cAA1D,CAAL,CACCkB,IADD,CACMC,QAAO,IAAKA,QAAQ,CAACC,IAAT,EADlB,EAECF,IAFD,CAEM,UAASlC,IAAT,EAAc;QACnBqC,OAAO,CAACC,GAAR,CAAY,UAAZ;QACAtC,IAAI,CAACuC,OAAL,CAAalB,OAAb,CAAqBmB,OAAM,IAAK;UAC/B,IAAGA,OAAO,CAACC,QAAR,KAAqB,MAAxB,EAAgCtC,QAAQ,CAAC0B,IAAT,GAAgBW,OAAO,CAACE,KAAxB;UAChC,IAAGF,OAAO,CAACC,QAAR,KAAqB,OAAxB,EAAiCtC,QAAQ,CAAC4B,KAAT,GAAiBS,OAAO,CAACE,KAAzB;UACjC,IAAGF,OAAO,CAACC,QAAR,KAAqB,YAAxB,EAAsCtC,QAAQ,CAAC6B,IAAT,GAAgBQ,OAAO,CAACE,KAAxB;QACtC,CAJD;QAKAd,IAAI,CAACvB,MAAL,CAAYI,MAAZ,CAAmB,QAAnB,EAA6BN,QAA7B;MACA,CAVD;IAWA,CA3CO;;IA4CRwC,aAA